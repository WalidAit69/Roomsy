{
    "buildCommand":"cd app && npm install && npx vite build",
    "outputDirectory":"app/dist",
    "framework":"vite",
    "rewrites":[
        {
            "source":"/api/(.*)",
            "destination":"/server/routes/route.js"
        },
        {
            "source":"/(.*)",
            "destination":"/server/routes/Placeroute.js"
        }
    ]
}